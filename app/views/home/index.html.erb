<!DOCTYPE html>
<html lang="en">

<head>

</head>

<body>

    

    <!-- Header Carousel -->
    <header id="myCarousel" class="carousel slide">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner">
            <div class="item active">
                <%= link_to image_tag("ssdDesign.jpg", :size => '1900x1080') %>
                <div class="carousel-caption">
                    <h2 style="text-align: right"><font size="12">Design</font size></h2>
                </div>
            </div>
            <div class="item">
                <%= link_to image_tag("ssdManufacture.png", :size => '1900x1080') %>
                <div class="carousel-caption">
                    <h2 style="text-align: right"><font size="12">Manufacture</font size></h2>
                </div>
            </div>
            <div class="item">
                <%= link_to image_tag("ssdEnjoy.png", :size => '1900x1080') %>
                <div class="carousel-caption">
                    <h2 style="text-align: right"><font size="12">Display</font size></h2>
                </div>
            </div>
        </div>

        <!-- Controls -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="icon-prev"></span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="icon-next"></span>
        </a>
    </header>

    <!-- Page Content -->
    <div class="container">

        <!-- Marketing Icons Section -->
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">
                    Welcome to Solid Solutions Design
                </h1>
            </div>
